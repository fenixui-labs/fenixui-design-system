        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@fenixui-org/tsconfig": "workspace:^",
        "@mui/material": "^5.14.6",
        "@types/react": "^18.2.21",
        "react": "^18.2.0",
        "tsup": "^6.7.0",
        "typescript": "^5.2.2",
        "valtio": "^1.11.2"
    },
    "devDependencies": {
        "rome": "^12.1.3"
    }
}