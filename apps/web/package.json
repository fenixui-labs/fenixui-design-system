{
  "name": "web",
  "version": "0.1.1",
  "scripts": {
    "reset": "npx rimraf ./**/node_modules",
    "dev": "modern dev",
    "build": "modern build",
    "start": "modern start",
    "serve": "modern serve",
    "new": "modern new",
    "lint": "modern lint",
    "upgrade": "modern upgrade",
    "test": "modern test"
  },
  "engines": {
    "node": ">=16.18.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "rome format --write",
      "rome check"
    ],
    "*.{js,jsx,mjs,mjsx,cjs,cjsx}": [
      "rome format --write",
      "rome check"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ],
  "dependencies": {
    "@modern-js/runtime": "2.32.1",
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  },
  "devDependencies": {
    "@modern-js-app/eslint-config": "2.32.1",
    "@modern-js/app-tools": "2.32.1",
    "@modern-js/builder-rspack-provider": "2.32.1",
    "@modern-js/eslint-config": "2.32.1",
    "@modern-js/plugin-testing": "2.32.1",
    "@modern-js/tsconfig": "2.32.1",
    "@types/jest": "~29.2.4",
    "@types/node": "~16.11.7",
    "@types/react": "~18.0.26",
    "@types/react-dom": "~18.0.10",
    "husky": "~8.0.1",
    "lint-staged": "~13.1.0",
    "prettier": "~2.8.1",
    "rimraf": "~3.0.2",
    "rome": "^12.1.3",
    "typescript": "~5.0.4"
  }
}